{
  "name": "raptor/test-utils",
  "description": "Testing utils for PHP",
  "license": "MIT",
  "require": {
    "php": ">=7.3.0",
    "symfony/console": "^3.0|^4.0",
    "mikey179/vfsstream": "1.6.*",
    "nesbot/carbon": "^2.19",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*",
    "mockery/mockery": "1.2.*",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "3.4.*",
    "roave/security-advisories": "dev-master",
    "escapestudios/symfony2-coding-standard": "3.9.*"
  },
  "autoload": {
    "psr-4": {
      "Raptor\\TestUtils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Raptor\\TestUtils\\UnitTests\\": "tests/unit",
      "Raptor\\TestUtils\\TestUtils\\": "tests/utils"
    }
  },
  "scripts": {
    "test-all": "phpunit"
  }
}
